    <meta charset="utf-8"></meta>
    <meta http-equiv="X-UA-Compatible" content="IE=edge"></meta>
    <title>{{name}}</title>
    <meta name="description" content=""></meta>
    <meta name="viewport" content="width=device-width, initial-scale=1"></meta>
    <apex:stylesheet value="{!URLFOR($Resource.@@resourceName, 'vendor.css')}" />
    <link rel="stylesheet" href="{!URLFOR($Resource.@@resourceName)}/app.css"></link>
  </head>
  <body>
    <script src="{!URLFOR($Resource.@@resourceName)}/vendor.js"></script>
